<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.james.reviewer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/testing"
        android:label="Agriculture Reviewer"
        android:roundIcon="@drawable/testing"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.LoginActivity"
            android:label="Reviewer"
                >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SignUpActivity"
            android:parentActivityName=".Activities.MainMenuActivity"
            android:label="Reviewer"/>
        <activity android:name=".Activities.MainMenuActivity"
            android:parentActivityName=".Activities.MainMenuActivity"/>
        <activity android:name=".Activities.QuizActivity"
            android:label="Review"/>
        <activity android:name=".Activities.QuizResultsActivity"
            android:parentActivityName=".Activities.MainMenuActivity"
            android:label="Results"/>
        <activity android:name=".Activities.SchedulesActivity"
            android:parentActivityName=".Activities.MainMenuActivity"/>
        <activity android:name=".Activities.AdminPanelActivity"
            android:label="Admin Panel"
            android:parentActivityName=".Activities.MainMenuActivity"/>
        <activity android:name=".Activities.ManageQuestionsActivity"
            android:parentActivityName=".Activities.MainMenuActivity"
            android:label="Manage Questions"/>
        <activity
            android:name=".Activities.RecordsActivity"
            android:label="Main Menu"
            android:parentActivityName=".Activities.MainMenuActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.Main2Activity"
            android:label="Main Menu"
            android:parentActivityName=".Activities.MainMenuActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.QuestionEditorActivity"
            android:parentActivityName=".Activities.MainMenuActivity"/>
        <activity android:name=".Activities.ViewUsersActivity"
            android:parentActivityName=".Activities.MainMenuActivity"/>
        <activity android:name=".Activities.ManageSchedulesActivity"></activity>
    </application>

</manifest>